{
  "custom/leftArrow": {
    "format": "ÔÑÑ",
    "tooltip": false,
    //TODO: Remove return-type/signal after performance bug is fixed
    "return-type": "json",
    "signal": 8
  },

  "custom/rightArrow": {
    "format": "ÔÑÖ",
    "tooltip": false,
    //TODO: Remove return-type/signal after performance bug is fixed
    "return-type": "json",
    "signal": 8
  },

  "group/tray": {
    "orientation": "horizontal",
    "drawer": {
      "transition-duration": 200,
      "transition-left-to-right": false,
      "click-to-reveal": false,
      "children-class": "group-tray"
    },
    "modules": ["custom/leftArrow", "tray"]
  },

  "group/taskbar": {
    "orientation": "horizontal",
    "drawer": {
      "transition-duration": 400,
      "transition-left-to-right": true,
      "click-to-reveal": false,
      "children-class": "group-tools"
    },
    "modules": ["custom/rightArrow", "wlr/taskbar"]
  },

  "group/tools": {
    "orientation": "horizontal",
    "drawer": {
      "transition-duration": 400,
      "transition-left-to-right": true,
      "click-to-reveal": false,
      "children-class": "group-tools"
    },
    "modules": ["custom/rightArrow", "custom/colorpicker", "custom/screenshot"]
  },

  "group/battery": {
    "orientation": "horizontal",
    "drawer": {
      "transition-duration": 400,
      "transition-left-to-right": false,
      "click-to-reveal": false,
      "children-class": "group-hardware"
    },
    "modules": ["battery", "backlight/slider", "power-profiles-daemon"]
  },

  "group/hardware": {
    "orientation": "horizontal",
    "drawer": {
      "transition-duration": 400,
      "transition-left-to-right": false,
      "click-to-reveal": false,
      "children-class": "group-hardware"
    },
    "modules": [
      "battery",
      "power-profiles-daemon",
      "cpu",
      "memory",
      "temperature",
      "backlight/slider"
    ]
  },

  "group/audio": {
    "orientation": "horizontal",
    "drawer": {
      "transition-duration": 400,
      "transition-left-to-right": false,
      "click-to-reveal": false,
      "children-class": "group-audio"
    },
    "modules": ["pulseaudio", "pulseaudio/slider"]
  },

  "pulseaudio": {
    "format": "<span size='11000' color='#FF8080'>{icon} </span> {volume}%",
    "tooltip-format": "{icon}   {desc} // {volume}%",
    "scroll-step": 5,
    "format-muted": "Ôë¶",
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "Ôñê",
      "headset": "Ôñê",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
    },
    // "on-click": "hyprpanel t audiomenu",
    "on-click": "pavucontrol",
    "on-click-right": "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle",
    "on-click-middle": "easyeffects"
  },

  "pulseaudio/slider": {
    "min": 5,
    "max": 100,
    "rotate": 0,
    "device": "pulseaudio",
    "scroll-step": 1
  },

  "group/network": {
    "orientation": "horizontal",
    "format": "ifname",
    "drawer": {
      "transition-duration": 400,
      "transition-left-to-right": false,
      "click-to-reveal": false,
      "children-class": "group-network"
    },
    "modules": ["network", "bluetooth"]
  },

