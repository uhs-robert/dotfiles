  "power-profiles-daemon": {
    "format": "<span size='11000'>{icon}</span>",
    "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
    "tooltip": true,
    "format-icons": {
      "default": "‚ö°",
      "performance": "‚ö°",
      "balanced": "üîã",
      "power-saver": "üå±",
    },
  },

  "cpu": {
    "format": "<span size='11000' foreground='#fab387'>Ôãõ </span>{usage}%",
    "interval": 2,
    "tooltip": true,
    "tooltip-format": "CPU Usage: {usage}%",
    "on-click": "kitty btop",
  },

  "memory": {
    "format": "<span size='11000' foreground='#b4befe'>ÓøÖ </span>{percentage}%",
    "interval": 2,
    "tooltip": true,
    "tooltip-format": "RAM Usage: {used:0.1f}gb / {total:0.1f}gb",
    "on-click": "kitty btop",
  },

  "temperature": {
    // "thermal-zone": 2,
    // "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
    "critical-threshold": 80,
    "format-critical": "<span size='11000' color='indianred'>Ôãâ</span> <span color='#FFA0A0'>{temperatureC}¬∞ C</span>",
    "format": "<span size='11000' color='#1CA1FF'>Ôãâ</span> {temperatureC}¬∞ C",
  },

  "backlight/slider": {
    "min": 0,
    "max": 100,
    "orientation": "horizontal",
    "device": "intel_backlight",
  },

  "clock": {
    "interval": 1,
    "format": "<span color='#6BBF59' size='11000'>Û∞∏ó </span>{:%a %b %d | %H:%M:%S}",
    // "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>"
    "tooltip-format": "\n<span size='9pt' font='WenQuanYi Zen Hei Mono'>{calendar}</span>",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 3,
      "weeks-pos": "right",
      "on-scroll": 1,
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "days": "<span color='#ecc6d9'><b>{}</b></span>",
        "weeks": "<span color='#99ffdd'><b>W{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b><u>{}</u></b></span>",
      },
    },
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down",
    },
    // "on-click": "hyprpanel t calendarmenu",
  },

  "network": {
    "format-wifi": "<span size='11000' foreground='#47A99B'>{icon} </span>{essid:.7}",
    "format-icons": ["Û∞§Ø", "Û∞§ü", "Û∞§¢", "Û∞§•", "Û∞§®"],
    "format-ethernet": "<span size='11000' foreground='#47A99B'>Û∞§≠ </span> Disconnected",
    "format-linked": "{ifname} (No IP) Ôûñ",
    "format-disconnected": "<span size='11000' foreground='#47A99B'>Û∞§≠ </span>Disconnected",
    "tooltip-format-wifi": "{essid}, {signalStrength}%",
    // "on-click": "hyprpanel t networkmenu",
    "on-click": "nm-connection-editor",
  },

  "bluetooth": {
    "format-on": "<span size='11000' color='#5BB0FA'>Û∞ÇØ On</span>",
    "format-off": "BT-off",
    "format-disabled": "Û∞Ç≤",
    "format-connected-battery": "<span size='11000' color='#5BB0FA'>Û∞ÇØ </span>Connected ({num_connections})",
    "format-alt": "{device_alias} <span size='11000' color='#5BB0FA'>Û∞ÇØ</span>",
    "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
    "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
    "tooltip-format-enumerate-connected": "{device_alias}\n{device_address}",
    "tooltip-format-enumerate-connected-battery": "{device_alias}\n{device_address}\n{device_battery_percentage}%",
    "on-click-right": "blueman-manager",
    // "on-click": "hyprpanel t bluetoothmenu",
  },

  "battery": {
    "states": {
      "warning": 30,
      "critical": 15,
    },
    "format": "<span size='11000' foreground='#FF9F41'>{icon} </span> {capacity}%",
    "format-warning": "<span size='11000' foreground='#FF9F41'>{icon} </span> {capacity}%",
    "format-critical": "<span size='11000' foreground='#CD5C5C'>{icon} </span> {capacity}%",
    "format-charging": "<span size='11000' foreground='#FF9F41'>Ôá¶ </span>{capacity}%",
    "format-plugged": "<span size='11000' foreground='#FF9F41'>Ôá¶ </span>{capacity}%",
    "format-alt": "<span size='11000' foreground='#FF9F41'>{icon} </span> {time}",
    "format-full": "<span size='11000' foreground='#FF9F41'>Û±ü¢ </span>{capacity}%",
    "format-icons": ["ÔâÑ", "ÔâÉ", "ÔâÇ", "ÔâÅ", "ÔâÄ"],
    "tooltip-format": "{time}",
    // "on-click": "hyprpanel t energymenu",
  },

  "pulseaudio": {
    "format": "<span size='11000' color='#FF8080'>{icon} </span> {volume}%",
    "tooltip-format": "{icon}   {desc} // {volume}%",
    "scroll-step": 5,
    "format-muted": "Ôë¶",
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "Ôñê",
      "headset": "Ôñê",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"],
    },
    // "on-click": "hyprpanel t audiomenu",
  },

  "pulseaudio/slider": {
    "min": 5,
    "max": 100,
    "rotate": 0,
    "device": "pulseaudio",
    "scroll-step": 1,
  },

  "custom/notifications": {
    "tooltip": true,
    "tooltip-format": "View notifications",
    "format": "<span size='11000'>{icon}</span>",
    "format-icons": {
      "notification": "Û±Ö´ ",
      "none": "ÔÉ≥ ",
      "inhibited-notification": "Û∞µô ",
      "dnd-notification": "Û∞Ö∏ ",
      "dnd-none": "Ôá∑ ",
      "dnd-inhibited-notification": "Û∞∫Å ",
    },
    "return-type": "json",
    "exec-if": "which swaync-client",
    "exec": "swaync-client -swb",
    "on-click": "swaync-client -t -sw",
    // "on-click": "hyprpanel t notificationsmenu",
    "on-click-right": "swaync-client -d -sw",
    "escape": true,
  },

  "tray": {
    "icon-size": 16,
    "spacing": 10,
    "hide-empty-text": true,
  },

  "custom/colorpicker": {
    "format": "<span size='11000'>Ôáª </span>",
    "tooltip": true,
    "tooltip-format": "Select a color",
    "on-click": "hyprpicker",
  },

  "custom/screenshot": {
    "format": "<span size='11000'>Û∞ªú </span>",
    "tooltip": true,
    "tooltip-format": "Take a screenshot",
    "on-click": "hyprshot -m region -o ~/Pictures/Screenshots",
  },

  "custom/startmenu": {
    "format": "<span size='15000'>Û∞£á</span>",
    "tooltip": true,
    "tooltip-format": "Launch an application",
    // "on-click": "hyprpanel t dashboardmenu",
    // "on-click": "wofi --show drun",
    "menu": "on-click",
    "menu-file": "/home/USER/.config/waybar/custom_modules/start_menu.xml",
    "menu-actions": {
      "allapps": "wofi --show drun",
      "web": "hyprctl dispatch exec firefox",
      "code": "kitty -e nvim",
      "files": "kitty -e yazi",
      "media": "mpv --player-operation-mode=pseudo-gui",
      "term": "hyprctl dispatch exec kitty",
      "settings": "systemsettings || gnome-control-center || true",

      "sep1": "true",

      "lock": "~/.config/waybar/scripts/confirm-action.sh --title Lock --glyph 'Û∞åæ' --color '#cdd6f4' --exec 'hyprlock'",
      "logout": "~/.config/waybar/scripts/confirm-action.sh --title Logout --glyph 'Û∞çÉ' --color '#89dceb' --exec 'hyprctl dispatch exit'",
      "reboot": "~/.config/waybar/scripts/confirm-action.sh --title Reboot --glyph 'Û∞úâ' --color '#f9e2af' --exec 'systemctl reboot'",
      "power": "~/.config/waybar/scripts/confirm-action.sh --title Power\\ Off --glyph 'Û∞ê•' --color '#f38ba8' --exec 'systemctl poweroff'",
    },
    "on-click-right": "wofi --show drun",
  },

  "wlr/taskbar": {
    "format": "{icon}",
    "icon-size": 14,
    "icon-theme": "Numix-Circle",
    "tooltip-format": "{title}",
    "on-click": "activate",
    "on-click-middle": "close",
    "ignore-list": ["Kitty"],
    "app_ids-mapping": {
      "firefoxdeveloperedition": "firefox-developer-edition",
    },
    "rewrite": {
      "Firefox Web Browser": "Firefox",
      "Foot Server": "Terminal",
    },
  },

  "mpris": {
    "format": "<span size='11000'>{player_icon}</span> {dynamic:.7}",
    "format-paused": "<span size='11000'>{status_icon}</span> <i>{dynamic}</i>",
    "max-length": 35,
    "player-icons": {
      "default": "‚ñ∂",
      "mpv": "üéµ",
      "spotify": "ÔÜº",
    },
    "status-icons": {
      "paused": "‚è∏",
    },
    "on-click": "playerctl play-pause",
    "on-click-middle": "playerctl previous",
    "on-click-right": "playerctl next",
    "tooltip-format": "{player} ({status})\n{title}\n{artist} - {album}\n[{position} / {length}]",
    // "ignored-players": ["firefox"]
  },

  "custom/cava": {
    "exec": "CAVA_BARS=10 CAVA_STYLE=blocks python3 ~/.config/waybar/scripts/cava_waybar.py",
    "return-type": "json",
    "interval": 0,
    "restart-interval": 1,
    "escape": true,
    "hide-empty-text": true,
    "format": "<span size='12000' color='#309DC9'>ÔÄÅ </span>{text}",
    "tooltip": false,
    // "on-click": "hyprpanel t mediamenu",
    "on-click": "playerctl play-pause",
    "on-click-middle": "playerctl previous",
    "on-click-right": "playerctl next",
  },

  "custom/updates": {
    "exec": "UPDATES_TTL_MIN=30 python3 ~/.config/waybar/scripts/fedora_updates.py",
    "return-type": "json",
    "interval": 1800, // in seconds = 30min
    "format": "<span size='11000' color='khaki'>ÔÇ≠ </span>{text}",
    "tooltip": false,
    "hide-empty-text": true,
    "on-click": "bash -lc 'kitty -e bash -lc \"sysup\"; rm -f \"$XDG_RUNTIME_DIR/fedora_updates.cache.json\"; pkill -RTMIN+8 waybar'",
    "on-click-right": "bash -lc 'kitty -e bash -lc \"sudo dnf5 upgrade --refresh -y || sudo dnf upgrade --refresh -y\"; rm -f \"$XDG_RUNTIME_DIR/fedora_updates.cache.json\"; pkill -RTMIN+8 waybar'",
  },

  "custom/weather": {
    "interval": 600, // in seconds is 10 minutes
    "exec": "python -O ~/.config/waybar/scripts/weather-api/get_weather_api.py",
    "markup": "pango",
    // "on-click": "hyprpanel t calendarmenu",
  },

  "custom/power": {
    "format": "‚èª ",
    "tooltip": false,
    "menu": "on-click",
    "menu-file": "~/.config/waybar/custom_modules/power_menu.xml",
    "menu-actions": {
      "lock": "hyprlock",
      "suspend": "systemctl suspend",
      "logout": "hyprctl dispatch exit",
      "reboot": "systemctl reboot",
      "poweroff": "systemctl poweroff",
    },
  },

  "custom/cc": {
    "format": "ÔÇÖ",
    "tooltip": false,
    "on-click": "ags toggle controlCenter",
  },
}
